
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("RentZestimates", "Home", "HomeOwner", new { area = "" }, new { @class = "navbar-brand" })
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@*@Html.ActionLink("Home", "Home", "HomeOwner")*@</li>
                <li>@Html.ActionLink("Login", "login", "HomeOwner")</li>
                <li>@Html.ActionLink("Register", "Register", "HomeOwner")</li>
                <li>@*@Html.ActionLink("Logout", "logOut", "HomeOwner")*@</li>
            </ul>
        </div>
    </div>
</div>

<div class="jumbotron">
    <h1>Welcome</h1>
    <p class="lead">Please  @Html.ActionLink("Login", "login") to get rent estimates for your location.</p>
    <p class="lead">Don't have an account?  @Html.ActionLink("Click here", "register") to register.</p>
</div>


